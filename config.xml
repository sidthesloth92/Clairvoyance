<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.clairvoyance" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Clairvoyance</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
      Ionic Framework Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="ios">
    <icon src="resources/ios/icon/icon.png" width="57" height="57"/>
    <icon src="resources/ios/icon/icon.png" width="114" height="114"/>
    <icon src="resources/ios/icon/icon.png" width="40" height="40"/>
    <icon src="resources/ios/icon/icon.png" width="80" height="80"/>
    <icon src="resources/ios/icon/icon.png" width="50" height="50"/>
    <icon src="resources/ios/icon/icon.png" width="100" height="100"/>
    <icon src="resources/ios/icon/icon.png" width="60" height="60"/>
    <icon src="resources/ios/icon/icon.png" width="120" height="120"/>
    <icon src="resources/ios/icon/icon.png" width="180" height="180"/>
    <icon src="resources/ios/icon/icon.png" width="72" height="72"/>
    <icon src="resources/ios/icon/icon.png" width="144" height="144"/>
    <icon src="resources/ios/icon/icon.png" width="76" height="76"/>
    <icon src="resources/ios/icon/icon.png" width="152" height="152"/>
    <icon src="resources/ios/icon/icon.png" width="29" height="29"/>
    <icon src="resources/ios/icon/icon.png" width="58" height="58"/>
    <icon src="resources/ios/icon/icon.png" width="87" height="87"/>
    <splash src="resources/ios/splash/splash.png" height="1136" width="640"/>
    <splash src="resources/ios/splash/splash.png" height="1334" width="750"/>
    <splash src="resources/ios/splash/splash.png" height="2208" width="1242"/>
    <splash src="resources/ios/splash/splash.png" height="1242" width="2208"/>
    <splash src="resources/ios/splash/splash.png" height="1536" width="2048"/>
    <splash src="resources/ios/splash/splash.png" height="768" width="1024"/>
    <splash src="resources/ios/splash/splash.png" height="2048" width="1536"/>
    <splash src="resources/ios/splash/splash.png" height="1024" width="768"/>
    <splash src="resources/ios/splash/splash.png" height="960" width="640"/>
    <splash src="resources/ios/splash/splash.png" height="480" width="320"/>
  </platform>
  <platform name="android">
    <icon src="resources/android/icon/icon.png" density="ldpi"/>
    <icon src="resources/android/icon/icon.png" density="mdpi"/>
    <icon src="resources/android/icon/icon.png" density="hdpi"/>
    <icon src="resources/android/icon/icon.png" density="xhdpi"/>
    <icon src="resources/android/icon/icon.png" density="xxhdpi"/>
    <icon src="resources/android/icon/icon.png" density="xxxhdpi"/>
    <splash src="resources/android/splash/splash.png" density="land-ldpi"/>
    <splash src="resources/android/splash/splash.png" density="land-mdpi"/>
    <splash src="resources/android/splash/splash.png" density="land-hdpi"/>
    <splash src="resources/android/splash/splash.png" density="land-xhdpi"/>
    <splash src="resources/android/splash/splash.png" density="land-xxhdpi"/>
    <splash src="resources/android/splash/splash.png" density="land-xxxhdpi"/>
    <splash src="resources/android/splash/splash.png" density="port-ldpi"/>
